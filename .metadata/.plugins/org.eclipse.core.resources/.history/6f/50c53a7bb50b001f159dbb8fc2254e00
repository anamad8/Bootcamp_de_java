import java.util.Scanner;

public class Ejercicio_1 {

	public static void main(String[] args) {
		/*
		 Hacer un programa que permita al usuario ingresae 5 n√∫meros y mostrarlos. 
		 */
		
		Scanner cargaDatos = new Scanner(System.in);
		
